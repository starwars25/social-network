<nav>
  <ul>
    <% if logged_in? %>
        <li><%= link_to 'Profile', profile_path %></li>
        <li><%= link_to 'Log out', logout_path, method: :delete %></li>
        <li><%= link_to 'Dialogs', dialogs_user_path(@current_user) %></li>
    <% else %>
        <li><%= link_to 'Register', signup_path %></li>
        <li><%= link_to 'Login', login_path %></li>
    <% end %>
  </ul>
</nav>